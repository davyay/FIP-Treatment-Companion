<VBox fx:id="vboxLayout" alignment="TOP_CENTER" prefHeight="499.0" prefWidth="535.0" spacing="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.davyay.fiptreatmentcompanion.CommunityLinksController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Community Links">
         <font>
            <Font size="42.0" />
         </font>
      </Text>
      <ImageView fitHeight="200.0" fitWidth="200.0">
         <image>
            <Image url="@Links.png" />
         </image>
      </ImageView>
        <Hyperlink onAction="#handleFIPWarriors" text="FIP Warriors Site" />
        <Hyperlink onAction="#handleFIPWarriorsFB" text="FIP Warriors Facebook" />
        <Hyperlink onAction="#handleFIPVets" text="FIP Vets" />
        <Button onAction="#handleHome" text="Home" VBox.vgrow="ALWAYS" />
    </children>
</VBox>
