<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<VBox fx:id="vboxLayout" alignment="CENTER" prefHeight="532.0" prefWidth="662.0" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="your.package.HomeScreenController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Home">
         <font>
            <Font size="50.0" />
         </font>
      </Text>
      <ImageView fitHeight="200.0" fitWidth="200.0">
         <image>
            <Image url="@res/Home.png" />
         </image>
      </ImageView>
        <HBox alignment="CENTER" spacing="10">
            <children>
                <Button fx:id="btnWeight" onAction="#handleWeight" text="Weight Tracker" />
                <Button fx:id="btnTreatment" onAction="#handleTreatment" text="Treatment" />
            </children>
        </HBox>
        <HBox alignment="CENTER" spacing="10" />
          <Button fx:id="btnExport" onAction="#handleExport" text="Export Data" />
          <Button fx:id="btnCommunity" onAction="#handleCommunity" text="Community Resources" />
    </children>
</VBox>
