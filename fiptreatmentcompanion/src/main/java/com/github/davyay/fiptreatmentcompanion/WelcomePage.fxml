<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" prefHeight="512.0" prefWidth="690.0" spacing="20" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="your.package.WelcomePageController">
    <padding>
        <Insets bottom="20" left="20" right="20" top="20" />
    </padding>
    <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Welcome to FIP Treatment Companion">
         <font>
            <Font size="35.0" />
         </font>
      </Text>
      <ImageView fitHeight="264.0" fitWidth="309.0">
         <image>
            <Image url="@res/FIPTCLogo.png" />
         </image>
      </ImageView>
        <Button onAction="#handleLoadProfile" text="Load Existing Cat Profile" />
        <Button onAction="#handleCreateProfile" text="Create New Cat Profile" />
    </children>
</VBox>
